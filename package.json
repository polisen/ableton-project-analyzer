  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "airbnb-typescript",
      "airbnb/hooks"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "no-console": ["error", { "allow": ["warn", "error", "debug"] }],
